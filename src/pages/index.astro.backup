<div class="support">
	<a href="https://twitter.com/DevLoop01" target="_blank"><i class="fab fa-twitter-square"></i></a>
	<a href="https://dribbble.com/devloop01" target="_blank"><i class="fab fa-dribbble"></i></a>
</div>
  
  </svg>
  <img src={Teeth.src} alt="Top Teeth" class="header-image"/>
   <Container>
    <h1 class="site-title">Maddie's Linktree</h1>
  <LinkGroup/>
  </Container>
</header>
  
  <footer class="site-footer">
    <img src={Teeth2.src} alt="Bottom Teeth" class="footer-image"/>
    <img src={Cat.src} alt="Cat" class="cat-image" id="catImage"/>
	<img src={Kirby.src} alt="Kirby" class="kirby-image" id="kirbyImage"/>
	<img src={Smiski.src} alt="Smiski" class="smiski-image" id="smiskiImage"/>
<img src={Cat2.src} alt="Cat2" class="cat2-image" id="cat2Image"/>
  </footer>
</Layout>


<script is:inline src="/scripts/stars.js"></script>

<script>
  const smiskiImage = document.getElementById('smiskiImage');
  let rotationDegrees = 0;
  
  if (smiskiImage) {
    smiskiImage.addEventListener('mouseenter', () => {
      rotationDegrees += 360;
      smiskiImage.style.transform = `scale(1.3) translateY(-20px) rotate(${rotationDegrees}deg)`;
      smiskiImage.style.transition = 'transform 0.6s cubic-bezier(0.34, 1.56, 0.64, 1)';
      smiskiImage.style.filter = 'brightness(1.2) drop-shadow(0 10px 20px rgba(0,0,0,0.3))';
      smiskiImage.style.animation = 'none';
    });
    
    smiskiImage.addEventListener('mouseleave', () => {
      smiskiImage.style.transform = `rotate(${rotationDegrees}deg)`;
      smiskiImage.style.filter = '';
      smiskiImage.style.transition = 'transform 0.5s cubic-bezier(0.4, 0, 0.2, 1), filter 0.5s ease';
      setTimeout(() => {
        smiskiImage.style.animation = 'smiski-pop 3s ease-in-out infinite';
      }, 500);
    });
    
    smiskiImage.addEventListener('click', () => {
      smiskiImage.style.animation = 'none';
      rotationDegrees += 720;
      smiskiImage.style.transform = `scale(1.5) rotate(${rotationDegrees}deg)`;
      smiskiImage.style.transition = 'transform 0.8s cubic-bezier(0.68, -0.55, 0.265, 1.55)';
      
      setTimeout(() => {
        smiskiImage.style.transform = `rotate(${rotationDegrees}deg)`;
        smiskiImage.style.transition = 'transform 0.5s ease';
        setTimeout(() => {
          smiskiImage.style.animation = 'smiski-pop 3s ease-in-out infinite';
        }, 500);
      }, 800);
    });
  }
</script>

<style>
  body {
    width: 100%;
    min-height: 100vh;
    background-color: #9dde35;
    background-image:
      radial-gradient(circle at top right, rgba(121, 68, 154, 0.13), transparent),
      radial-gradient(circle at 20% 80%, rgba(41, 196, 255, 0.13), transparent);
    margin: 0;
    overflow-x: hidden;
    animation: squig-l-vision 0.6s infinite;
  }

  canvas#stars {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    z-index: -1;
  }

  .header-image {
    width: 100%;
    height: auto;
    display: block;
    margin: 0;
	margin-top: -150px;
  }

  .site-footer {
    width: 100%;
    margin-top: 2rem;
    position: relative;
  }

  .footer-image {
    width: 100%;
    height: auto;
    display: block;
    margin: 0;
  }

  .cat-image {
    position: absolute;
    bottom: 0;
    right: 0;
	left: -100px;
    width: 500px;
    height: auto;
    z-index: 10;
	top: -300px;
    pointer-events: none;
    animation: cat-pop 2s ease-in-out infinite;
  }

  .smiski-image {
    position: absolute;
    bottom: 0;
    right: 90px;
    width: 200px;
    height: auto;
    z-index: 10;
    top: -120px;
    pointer-events: auto;
    cursor: pointer;
    animation: smiski-pop 3s ease-in-out infinite;
    transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
  }

  .kirby-image {
    position: absolute;
    bottom: 0;
    right: 190px;
    width: 150px;
    height: auto;
    z-index: 11;
    top: -270px;
    pointer-events: none;
    animation: kirby-pop 3s ease-in-out infinite;
  }
  .cat2-image {
    position: absolute;
    bottom: 0;
    right: 600px;
    width: 200px;
    height: auto;
    z-index: 10;
    top: -40px;
    pointer-events: none;
   
  }

  @keyframes cat-pop {
    0%, 100% {
      transform: scale(1) rotate(-3deg);
    }
    50% {
      transform: scale(1.05) rotate(3deg);
    }
  }

  @keyframes smiski-pop {
    0%, 100% {
      transform: scale(1) rotate(3deg);
    }
    50% {
      transform: scale(1.05) rotate(-3deg);
    }
  }

  @keyframes kirby-pop {
    0%, 100% {
      transform: scale(1) rotate(-5deg) translateZ(0);
    }
    40% {
      transform: scale(1.05) rotate(-5deg) translateZ(0);
    }
    45% {
      transform: scale(0.95) rotate(5deg) translateZ(0);
    }
    50% {
      transform: scale(1.2) rotate(5deg) translateZ(0);
    }
  }

  h1 {
    color: #005490;
    margin-bottom: 1.5rem;
    font-family: Arial, sans-serif;
  }

  :global(a) {
    color: #005490;
    text-decoration: none;
  }

  :global(a:hover) {
    color: #003d6b;
  }
  
  * {
	box-sizing: border-box;
	margin: 0;
	padding: 0;
  }

  @keyframes squig-l-vision {
	0% {
		-webkit-filter: url("#squiggly-0");
		filter: url("#squiggly-0");
	}
	25% {
		-webkit-filter: url("#squiggly-1");
		filter: url("#squiggly-1");
	}
	50% {
		-webkit-filter: url("#squiggly-2");
	}
	75% {
		-webkit-filter: url("#squiggly-3");
		filter: url("#squiggly-3");
	}
	100% {
		-webkit-filter: url("#squiggly-4");
		filter: url("#squiggly-4");
	}
  }
.site-title {
  text-align: center;
  font-size: 2.5rem;
  margin-bottom: 2rem;
  .support {
	position: absolute;
	right: 10px;
	bottom: 10px;
	padding: 10px;
	display: flex;

	a {
		margin: 0 10px;
		color: #000;
		font-size: 1.8rem;
		backface-visibility: hidden;
		transition: all 150ms ease;
		&:hover {
			transform: scale(1.1);
		}
	}
  }
</style>

